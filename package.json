{
  "name": "kmobnb-frontend",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:remote": "npm run dev",
    "dev:remote:mac": "npm run dev",
    "dev:local": "set VITE_LOCAL=true&&vite",
    "dev:local:mac": "export VITE_LOCAL=true && vite",
    "build": "vite build",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "sass-dev": "sass --watch --update --style=expanded src/assets/styles:src/assets/css.local",
    "sass-prod": "sass --no-source-map --style=compressed src/assets/styles:src/assets/dist"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "axios": "^1.5.0",
    "date-fns": "^4.1.0",
    "react": "^18.2.0",
    "react-date-range": "^2.0.1",
    "react-dom": "^18.2.0",
    "react-multi-carousel": "^2.8.5",
    "react-redux": "^8.1.2",
    "react-remove-scroll": "^2.6.3",
    "react-router": "^6.16.0",
    "react-router-dom": "^6.16.0",
    "redux": "^4.2.1",
    "socket.io-client": "^4.7.2"
  },
  "devDependencies": {
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "eslint": "^8.45.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "sass": "^1.67.0",
    "vite": "^4.4.5"
  }
}
